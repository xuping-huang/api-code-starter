{
  "name": "code-api-starter",
  "version": "1.0.0",
  "description": "a starter",
  "main": "build/main/main.js",
  "typings": "build/main/main.d.ts",
  "repository": "https://github.com/xuping-huang/code-api-starter",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "js": "set DEBUG=code:generator&&set NODE_ENV=development&&set CODE_ENV=eslint,jwt,jsondb,javascript&&ts-node src/main.ts",
    "dynamo": "set DEBUG=code:generator&&set NODE_ENV=development&&set CODE_ENV=eslint,jwt,m2m,test,cov,aws,dynamo,javascript,name&&ts-node src/main.ts",
    "sqlite": "set DEBUG=code:generator&&set NODE_ENV=development&&set CODE_ENV=eslint,jwt,test,cov,sqlite,javascript,name&&ts-node src/main.ts",
    "swagger": "set DEBUG=code:name&&set CONFIG_ENV=sample-meta.yml&&set NODE_ENV=development&&ts-node src/main.ts",
    "angular": "set DEBUG=code:generator&&set CONFIG_ENV=angular2.yml&&set CODE_ENV=name,bootstrap&&ts-node src/main.ts",
    "describe": "npm-scripts-info",
    "build:main": "tsc -p tsconfig.json",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "trash build test"
  },
  "engines": {
    "node": "10.15.1"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "dotenv": "^6.0.0",
    "joi": "^14.3.0",
    "js-yaml": "^3.12.1",
    "merge-deep": "^3.0.2",
    "mustache": "^3.0.1",
    "node-yaml-config": "^0.0.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.11.4",
    "@types/debug": "^0.0.31",
    "@types/dotenv": "^6.0.0",
    "@types/joi": "^14.0.1",
    "@types/mustache": "^0.8.32",
    "@types/node": "^9.6.41",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.0",
    "typescript": "^3.1.6"
  },
  "prettier": {
    "singleQuote": true
  }
}
